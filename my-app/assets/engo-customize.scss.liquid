.horizon-menu-home{
  background: #fff;
  width: 440px;
  max-width: 100%;
  position: fixed;
  top: 0;
  padding: 80px 50px 80px 120px;
  z-index: 5;
  bottom: 0;
  box-shadow: 0 5px 20px rgba(0, 0, 0, .1);
  left: 0;
  transition: transform .8s cubic-bezier(.46,.01,.32,1);
  transform: translateX(-100%);

  ::-webkit-scrollbar {
    width: 3px;
  }
  ::-webkit-scrollbar-track {
    background: #fff; 
  }
  ::-webkit-scrollbar-thumb {
    background: #ddd;
    border-radius: 10px;
  }
  ::-webkit-scrollbar-thumb:hover {
    background: #ccc; 
  }
  .close-menu-horizon {
    font-size: 20px;
    font-weight: 700;
    display: inline-block;
    cursor: pointer;
    margin-bottom: 45px;
    transition: all 0.5s ease;
    &:hover {
      color: $color-main2;
      transform: rotate(180deg);
    }
  }
  .main-navigation {
    overflow-y: auto;
    overflow-x: hidden;
    height: calc(100vh - 200px) !important;
    .menu-horizon-list{
      .nammenu{
        font-size: 18px;
        width: calc(100% - 50px);
        font-weight: 400;
        height: 55px;
        color: $color-main;
        display: inline-block;
        transition: 0.5s;
        text-decoration: none;
        text-transform: uppercase;
        position: relative;
        &:before{
          content: attr(title);
          color: $color-main;
          position: absolute;
          top: 0;
          left: 0;
          bottom: 0;
          right: 0;
          height: 100%;
          transition: height 1s ease 0s;
          overflow: hidden;
        }
        &:hover{
          color:$color-main2;
          &:before{
            height: 0;
          }
        }
        .label_menu {
          top: -10px;
          left: 90%;
        }

      }
      .toggle-menumobile {
        width: 50px;
        height: 48px;
        text-align: center;
        float: right;
        text-decoration: none;
        i{
          display: block;
          font-size: 25px;
          transition: 0.5s;
          color: $color-main;
        }
        &.active{
          i{
            transform: rotate(180deg);
            color:$color-main2;
          }
        }
      }
      .title_menu_mb{
        font-size: 14px;
        letter-spacing: 0.2em;
        font-weight: 500;
        text-transform: uppercase;
        position: relative;
        padding-bottom: 13px;
        margin-top: 20px;
        &:before {
          position: absolute;
          content: '';
          width: 50px;
          border-bottom: 1px solid $color-main2;
          bottom: 0;
        }
        &:first-child {
          margin-top: 0px;
        }
      }
      ul{
        list-style-type: none;
        &:last-child {
          margin-bottom: 20px !important;
        }
        li{
          line-height: 24px;
          position: relative;
          padding: 6px 0;
          a{
            font-size: 14px;
            line-height: 24px;
            font-weight: 400;
            color: $color-main;
            text-transform: unset;
            .label_menu {
              top: -16px;
            }
          }
        }
      }
      .title_menu_mb.title_only_one{          
        font-size: 14px;
        line-height: 24px;
        font-weight: 400;
        color: $color-main;
        letter-spacing: unset;
        text-transform: unset;
        margin-top: unset;
        &:before {
          content: none;
        }
        &:last-child {
          margin-bottom: 20px;
        }

      }

    }

  }
  &.active{
    transform: unset;
  }
}

.bg-horizon-menu{
