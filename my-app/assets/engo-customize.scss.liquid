#back-to-top {
  position: fixed;
  bottom: 30px;
  right: 15px;
  width: 40px;
  border-radius:50px;
  height: 40px;
  text-align: center;
  line-height: 30px;
  background: #fff;
  cursor: pointer;
  text-decoration: none;
  transition: all 0.5s ease 0s;
  transform: translate(80px, 0px);
  z-index: 2;
  box-shadow: 1px 2px 6px 4px rgba(53, 57, 68, 0.2);
  i {
    color: $color-main2;
    display: inline-block;
    line-height: 40px;
  }
  &.show{
    transform: translate(0px, 0px);
  }
  &:hover{
    background-color: $color-main2;
    i {
      color: #fff;

    }
  }
}

.section-slideshow-v5 {
  position: relative;
  overflow: hidden;
  button.next-slide,
  button.prev-slide {
    position: absolute;
    top: 50%;
    z-index: 1;
    transform: translateY(-50%);
    width: 50px;
    height: 50px;
    line-height: 50px;
    border-radius: 50%;
    text-transform: center;
    font-size: 22px;
    background: #fff;
    color: $color-main;
    transition: all .5s ease;
    box-shadow: 0px 2px 10px -2px rgba(0,0,0,0.3);
    &:hover {
      color: #fff;
      background-color: $color-main2;
    }
  }
  button.next-slide {
    right: -50px;
    opacity: 0;

  }
  button.prev-slide {
    left: -50px;
    opacity: 0;
  }
  &:hover {
    button.next-slide {
      right: 15px;
      opacity: 1;
    }
    button.prev-slide {
      left: 15px;
      opacity: 1;
    }
  }
  .slick-dots{
    transform: translate(-50%);
    left: 50%;
    bottom: 54px;
    li{
      margin: 0 3px;
      button{
        &:before{
          opacity: 0;
        }
        font-size: unset;
        line-height: 20px;
        position: absolute;
        top: 0;
        left: 0;
        width: 12px;
        height: 12px;
        border: 1px solid #c9c5bf;
        border-radius: 50%;
      }
    }
    .slick-active{
      button{
        background-color: #000000;
        border-radius: 50%;
      }
    }
  }
  .slideshow-item {
    position: relative;
  }
  .bg-slider {
    img {
      width: 100%;
      object-fit: cover;
      @media screen and (max-width: 991px) {
        height: 600px;
      }
      @media screen and (max-width: 767px) {
        height: 500px;
      }
      @media screen and (max-width: 575px) {
        height: 500px;
      }
    }
  }

  .slideshow-content {
    position: absolute;
    top: 30%;
    left: 50%;
    transform: translateX(-50%);
    width: 100%;
    @media screen and (max-width: 1399px) {
      top: 20%;
    }
    @media screen and (max-width: 1024px) {
      top: 16%;
    }
    @media screen and (max-width: 1199px) {
      top: 50%;
      transform: translate(-50%,-50%);
    }
    .content2 {
      font-size: 60px;
      font-weight: 700;
      letter-spacing: 1px;
      line-height: 1;
      color: #fff;
      margin-bottom: 15px;
      display: inline-block;
      text-transform: uppercase;
      transform: translateX(-300px);
      opacity: 0;
      visibility: hidden;
      transition: .5s linear 1s;
      span {
        transition: .05s;
        display: inline-block;
      }
      strong {
        color: $color-main;
        font-weight: 700;
      }
      @media screen and (max-width: 1199px) {
        font-size: 40px;
      }
      @media screen and (max-width: 991px) {
        font-size: 36px;
        margin-bottom: 15px;
      }
      @media screen and (max-width: 767px) {
        font-size: 32px;
        margin-bottom: 20px;
      }
      @media screen and (max-width: 575px) {
        font-size: 30px;
        margin-bottom: 20px;
      }
    }
    .content3 {
      font-size: 40px;
      font-weight: 500;
      letter-spacing: 1px;
      line-height: 1;
      color: #fff;
      margin-bottom: 40px;
      text-transform: uppercase;
      transform: translateX(300px);
      opacity: 0;
      visibility: hidden;
      transition: .5s linear 1.4s;

      @media screen and (max-width: 1199px) {
        font-size: 30px;
      }
      @media screen and (max-width: 1024px) {
        font-size: 20px;
        margin-bottom: 25px;
      }
      @media screen and (max-width: 767px) {
        font-size: 18px;
        margin-bottom: 15px;
      }
      @media screen and (max-width: 575px) {
        font-size: 16px;
      }
      strong {
        color: $color-main;
        font-weight: 500;
      }
    }
    .btn-slideshow {
      padding: 18px 50px;
      border-radius: 50px;
      font-size: 15px;
      line-height: 1;
      font-weight: 600;
      font-family: $font-primary;
      text-transform: uppercase;
      margin-top: 20px;
      display: inline-block;
      transition: all 0.5s ease; 
      transform: translateY(50px);
      opacity: 0;
      @media screen and (max-width: 992px) {
        font-size: 14px;
        padding: 14px 40px;
      }
      @media screen and (max-width: 576px) {
        font-size: 12px;
        padding: 12px 30px;
      }
      &:hover, &:focus {
        background-color: #000;
      }
    }
    &.active {
      .label-slider, .content1, .content2, .content3, .content4, .btn-slideshow {
        opacity: 1;
        visibility: visible;
        transform: none;
      }
    }
  }
}

