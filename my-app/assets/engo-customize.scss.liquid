.section-slideshow-v6 {
  overflow: hidden;
  button.next-slide,
  button.prev-slide {
    position: absolute;
    top: 50%;
    z-index: 1;
    transform: translateY(-50%);
    width: 50px;
    height: 50px;
    line-height: 50px;
    border-radius: 50%;
    text-transform: center;
    font-size: 22px;
    background: #fff;
    color: $color-main;
    transition: all .5s ease;
    box-shadow: 0px 2px 10px -2px rgba(0,0,0,0.3);
