.list-category{
  padding: 50px 0;
  .img_collection{
    position: relative;
    overflow: hidden;
    img{
      transition : .3s;
    }
    &:before, &:after{
      content: '';
      position: absolute;
      top: 0;
      bottom: 0;
      right: 0;
      left: 0;
      -webkit-transition: all 0.6s ease;
      -moz-transition: all 0.6s ease;
      -o-transition: all 0.6s ease;
      transition: all 0.6s ease;
      z-index: 1;
    }
    &:hover{
      img{
        transform: scale(1.05);
        transition : .3s;
      }
      &:before{
        right: 50%;
        left: 50%;
        background-color: rgba(255,255,255,0.5);
      }
      &:after{
        top: 50%;
        bottom: 50%;
        background-color: rgba(255,255,255,0.5);
