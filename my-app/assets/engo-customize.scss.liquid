      text-align: center;
      line-height: 1.2;
      transform:  translate(-50%, -50%) rotate(45deg) scale(0);
      left: 50%;
      top : 50%;
      transition: all .3s ease;
      background-color: #ff66015e;
      padding : 10px;
      border-radius : 20px;
      &:hover{
        transition: all .3s ease;
        a{
          color: $color-main2;
        }
      }
    }
  }
  .product-icon-action{
    @media(max-width: 767px){
      display: none;
    }
    position: absolute;
    bottom: 10px;
    left: 0;
    right: 0;
    background-color: transparent;
    -webkit-transition: .4s linear;
    -moz-transition: .4s linear;
    -ms-transition: .4s linear;
    -o-transition: .4s linear;
    transition: .4s linear;
    display: flex;
    display: -webkit-flex;
    display: -moz-flex;
    display: -ms-flex;
    flex-wrap: wrap;
    justify-content: center;
    .add-wishlist, .add-to-cart, .quick-view{
      transform: translateY(30px);
      transition: .5s;
      visibility: hidden;
      opacity: 0;
      a, button{
        color: #232529;
        display: inline-block;
        width: 46px;
        height: 46px;
        line-height: 46px;
        text-align: center;
        border-radius: 50%;
        padding: 0;
        font-weight: 400;
        margin: 0 10px;
        background-color: #fff;
        font-size: 0;
        box-shadow: 0 3px 10px rgba(0,0,0,.08);
        transition: .3s;
        i{
          font-size: 18px;
          color: #000;
          display: flex;
          justify-content: center;
          line-height: 46px;
          transition: .3s;
        }
        svg{
          fill: #000;
          width: 25px;
        }

        &:hover{
          background-color: $color-main2;
          transition: .3s;
          i{
            color: #fff;
            z-index: 1;
            transition: .3s;
          }
          svg{
            fill: #fff;
          }
        }
      }
    }
  }
  &:hover{
    .sold-out{
      transform:  translate(-50%, -50%) rotate(45deg) scale(1);
      transition: all .3s ease;
    }
    .product-icon-action{
      .add-wishlist, .add-to-cart, .quick-view{
        transform: translateY(0);
        transition: .5s;
        opacity: 1 ;
        visibility: visible;
      }
      .add-wishlist{
        transition-delay: 0.1s;
      }
      .add-to-cart{
        transition-delay: 0.2s;
      }
      .quick-view{
        transition-delay: 0.3s;
      }
    }
  }
}

